.row
    .col-lg-6.col-lg-offset-3
        ul.breadcrumb
            li Entities
.row(ng-init="init()")
    .col-lg-2
        button.btn.btn-info(type="submit",ng-click="createEntity()") ADD ENTITY
    .col-lg-10
        table.table(ng-table="entitiesTableParams", show-filter="true")
            tr(ng-repeat="entity in $data")
                td(title="name", filter="{ name: 'text'}",sortable="'name'")
                    a(ng-href="entities/{{entity.uuid}}") {{entity.name}}
                td(title="valueType") {{entity.valueType}}