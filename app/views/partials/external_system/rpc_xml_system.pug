form(role="form",name="helpForm", ng-if="external_system.customAttributes.system_id===1")
    div.form-group
        label Entity Type Name
        input.form-control(type="text", data-ng-model="item[item_key].name")
    div.form-group
        label Write Model
        input.form-control(type="text", data-ng-model="item[item_key].write.model")
    div.form-group
        label Write Page Size
        input.form-control(type="number", data-ng-model="item[item_key].write.pageSize")
    div.form-group
        label Read Model
        input.form-control(type="text", data-ng-model="item[item_key].read.model")
    div.form-group
        label Read Page Size
        input.form-control(type="number", data-ng-model="item[item_key].read.pageSize")
    div.form-group
        label Trigger Type
        select.form-control( ng-model="item[item_key].trigger",
        ng-options="trigger for trigger in triggers")
    div.btn-group
        button.btn.btn-default(type="submit", ng-click="saveEntityTypeEntry(item)") SAVE
        button.btn.btn-default(type="submit", ng-click="emptyEntityTypeEntry()") Empty